---
layout: default
---

<% for section, index in @sections : %>
  <section class="section <%= section %>" id="<%= section %>" data-section>
    <% if (index % 2) : %>
      <div class="wrapper">
        <%- @partial("html/section/#{section}.html.eco", @) %>
      </div>
    <% else : %>
      <%- @partial("html/section/#{section}.html.eco", @) %>
    <% end %>
  </section>
<% end %>
